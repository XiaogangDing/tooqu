<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
    <rule>
        <from>^/v[\d\.]+/jsmin/(.*)\.js$</from>
        <to>/app/jsmin?f=$1</to>
    </rule>
    <rule>
        <from>^/v[\d\.]+/jsgroup/(.*)\.js$</from>
        <to>/app/jsgroup?f=$1</to>
    </rule>
    <rule>
        <from>^/v[\d\.]+/css/(.*)\.min\.css$</from>
        <to>/app/cssmin?f=$1</to>
    </rule>
    <rule>
        <from>^/v[\d\.]+/css/(.*)\.group\.css$</from>
        <to>/app/cssmin?f=$1</to>
    </rule>
    <rule>
        <from>^/v[\d\.]+/images/(.*)$</from>
        <to>/images/$1</to>
    </rule>
    <rule>
        <from>^/v[\d\.]+/css/(.*)$</from>
        <to>/css/$1</to>
    </rule>
    <rule>
        <from>^/v[\d\.]+/js/(.*)$</from>
        <to>/js/$1</to>
    </rule>
    <rule>
        <from>^/images/(.*)$</from>
        <to>/images/$1</to>
    </rule>
    <rule>
        <from>^/css/(.*)$</from>
        <to>/css/$1</to>
    </rule>
    <rule>
        <from>^/js/(.*)$</from>
        <to>/js/$1</to>
    </rule>
    <rule>
        <from>^/s/ci$</from>
        <to>/app/static/ci?f=not_found.png</to>
    </rule>
    <rule>
        <from>^/s/(.*)$</from>
        <to>/app/static?f=$1</to>
    </rule>
    <rule>
        <from>^/(.*)$</from>
        <to>/app/$1</to>
    </rule>
    <outbound-rule>
        <from>^/app/(.*)$</from>
        <to>/$1</to>
    </outbound-rule>
</urlrewrite>